<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="kh.org.childhelpline.youthhealth">

    <!-- You will only need to add this meta-data tag, but make sure it's a child of application -->
       <meta-data
         android:name="com.google.android.geo.API_KEY"
         android:value="_YOUR_API_KEY_"/>

    <uses-permission android:name="android.permission.INTERNET" />

    <application
      android:name=".MainApplication"
      android:label="@string/app_name"
      android:icon="@mipmap/ic_launcher"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:allowBackup="false"
      android:theme="@style/AppTheme">
      <activity
        android:name=".MainActivity"
        android:label="@string/app_name"
        android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode"
        android:launchMode="singleTask"
        android:windowSoftInputMode="adjustResize"
        android:screenOrientation="nosensor"
        android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        <intent-filter>
          <action android:name="android.intent.action.VIEW" />
          <category android:name="android.intent.category.DEFAULT" />
          <category android:name="android.intent.category.BROWSABLE" />
          <data
            android:scheme="https"
            android:host="youthhealth-stg.ilabsea.org"
          />
          <data
            android:scheme="http"
            android:host="youthhealth-stg.ilabsea.org"
          />
          <data
            android:scheme="https"
            android:host="youthhealth-stg.childhelplinecambodia.org"
          />
          <data
            android:scheme="http"
            android:host="youthhealth-stg.childhelplinecambodia.org"
          />
          <data
            android:scheme="https"
            android:host="youthhealth.childhelplinecambodia.org"
          />
          <data
            android:scheme="http"
            android:host="youthhealth.childhelplinecambodia.org"
          />
          <data
            android:scheme="https"
            android:host="youthhealth-stg.childhelpline.org.kh"
          />
          <data
            android:scheme="http"
            android:host="youthhealth-stg.childhelpline.org.kh"
          />
          <data
            android:scheme="https"
            android:host="youthhealth.childhelpline.org.kh"
          />
          <data
            android:scheme="http"
            android:host="youthhealth.childhelpline.org.kh"
          />
        </intent-filter>
      </activity>
      <meta-data
        android:name="com.google.android.geo.API_KEY"
        android:value=""/>
    </application>
</manifest>
